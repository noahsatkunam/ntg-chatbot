{
  "name": "chatbot-platform-backend",
  "version": "1.0.0",
  "description": "Multi-tenant chatbot platform backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:reset": "prisma migrate reset",
    "db:setup": "npm run prisma:generate:dev && npm run prisma:push:dev && npm run prisma:seed",
    "prisma:generate:dev": "prisma generate --schema=./prisma/schema.dev.prisma",
    "prisma:push:dev": "prisma db push --schema=./prisma/schema.dev.prisma",
    "prisma:db:push": "prisma db push",
    "migrate:re-encrypt-api-connections": "tsx scripts/reEncryptApiConnections.ts",
    "migrate:re-encrypt-oauth2": "tsx scripts/reEncryptOAuth2Data.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.9.1",
    "@prisma/client": "^5.8.0",
    "@qdrant/js-client-rest": "^1.7.0",
    "@supabase/supabase-js": "^2.39.0",
    "archiver": "^6.0.1",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.0",
    "express-validator": "^7.0.1",
    "file-type": "^18.5.0",
    "form-data": "^4.0.4",
    "helmet": "^7.1.0",
    "isomorphic-dompurify": "^2.9.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.1.25",
    "mammoth": "^1.6.0",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-clamav": "^1.0.11",
    "nodemailer": "^6.9.13",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1",
    "pino": "^8.19.0",
    "pino-http": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "rate-limit-redis": "^4.2.0",
    "rate-limiter-flexible": "^3.0.0",
    "redis": "^4.6.12",
    "sharp": "^0.32.6",
    "socket.io": "^4.7.2",
    "speakeasy": "^2.0.0",
    "stripe": "^14.21.0",
    "tiktoken": "^1.0.10",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.11.0",
    "xss": "^1.0.15",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.0",
    "@types/nodemailer": "^6.4.14",
    "@types/pdf-parse": "^1.1.4",
    "@types/redis": "^4.0.10",
    "@types/sharp": "^0.32.0",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@types/validator": "^13.11.7",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.2.0",
    "prisma": "^5.8.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
